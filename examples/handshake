    ! receive $double : n_rch .
    let n   = <- n_rch .
    let rch = -> n_rch .
    send rch : n * 2 .
    end

|

    ! receive $addone_then_double : n_rch .
    let n   = <- n_rch .
    let rch = -> n_rch .
    new rch_d .
    send $double : { n + 1 , rch_d } .
    receive rch_d : res .
    send rch : res .
    end

|

    new rch .
    send $addone_then_double : { 10 , rch } .
    receive rch : n .
    send $print : n .
    end
